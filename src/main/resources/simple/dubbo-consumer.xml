<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	   http://code.alibabatech.com/schema/dubbo
	     http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="dubboDemo"/>
    <dubbo:registry address="zookeeper://localhost:2181" timeout="30000"/>
    <!--dubbo 监控 dubbox 改进版本中有用到-->
    <!--<dubbo:monitor protocol="registry" enable="${dubbo.monitor.enable}"/>-->
    <dubbo:protocol name="dubbo" port="20880" serialization="kryo"/>

    <dubbo:reference interface="me.myProjects.dubbo.service.DubboService" check="false" scope="true" id="dubboRpcService"/>
</beans>
